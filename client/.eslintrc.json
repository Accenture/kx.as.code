{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "MAIN_WINDOW_WEBPACK_ENTRY": true
  },
  "plugins": [
    "react"
  ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/react",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js",".jsx"]
      }
    }
  },
  "rules": {
    "react/prop-types": 0
  }
}
